{
    bottomWall
    {
        type            fixedValue;
        value           uniform 305;
    }

    movingWall
    {
        type            fixedValue;
        value           uniform 300;
    }

    fixedWalls
    {
        type            zeroGradient;
    }

    frontAndBack
    {
        type            empty;
    }

    #includeEtc "caseDicts/setConstraintTypes"
